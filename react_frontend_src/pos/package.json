{
  "name": "pos",
  "version": "1.0.0",
  "description": "OpenStudio Point of Sale",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/jest/bin/jest.js",
    "coverage": "node_modules/jest/bin/jest.js --coverage",
    "start": "webpack-dev-server --config webpack.config.dev.js --mode development --open",
    "build": "webpack --mode production --config webpack.config.prod.js",
    "build_dev": "webpack --mode development --config webpack.config.dev.js"
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "collectCoverageFrom": [
      "**/src/**.{js,jsx}",
      "!**/node_modules/**",
      "!**/src/admin_lte_dev/**",
      "!**/vendor/**"
    ],
    "notify": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/admin_lte_dev"
    ],
    "modulePathIgnorePatterns": [
      "shim.js",
      "global.js"
    ],
    "setupFiles": [
      "./__tests__/shim.js",
      "./__tests__/global.js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "author": "OpenStudio team",
  "license": "GPL-2.0",
  "dependencies": {
    "axios": "^0.18.0",
    "formik": "^1.5.1",
    "inputmask": "^4.0.6",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.4.1",
    "npm": "^6.14.18",
    "react": "^16.4.1",
    "react-confirm-alert": "^2.4.1",
    "react-dom": "^16.4.1",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-toastify": "^5.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2",
    "validator": "^10.7.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "create-react-class": "^15.6.3",
    "css-loader": "^0.28.11",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.9.0",
    "html-webpack-plugin": "^3.2.0",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.13.0",
    "npm-check": "^5.9.0",
    "postcss-loader": "^2.1.5",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.14"
  }
}
